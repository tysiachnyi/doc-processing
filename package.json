    "dev": "tsx watch src/server.ts",
    "worker:dev": "tsx watch src/worker.ts",
    "build": "npm run format:check && npm run lint && rimraf dist .tsbuildinfo && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "worker:start": "node dist/worker.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --write .",
    "test": "tsx --test test"
